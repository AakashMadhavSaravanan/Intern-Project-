# List operations
my_list = [1, 2, 3]
print("Initial list:", my_list)

# Adding elements to the list
my_list.append(4)
print("After appending 4:", my_list)

# Removing elements from the list
my_list.remove(2)
print("After removing 2:", my_list)

# Modifying elements in the list
my_list[0] = 10
print("After modifying the first element to 10:", my_list)

# Dictionary operations
my_dict = {'a': 1, 'b': 2, 'c': 3}
print("\nInitial dictionary:", my_dict)

# Adding elements to the dictionary
my_dict['d'] = 4
print("After adding key 'd' with value 4:", my_dict)

# Removing elements from the dictionary
del my_dict['b']
print("After deleting key 'b':", my_dict)

# Modifying elements in the dictionary
my_dict['a'] = 10
print("After modifying value of key 'a' to 10:", my_dict)

# Set operations
my_set = {1, 2, 3}
print("\nInitial set:", my_set)

# Adding elements to the set
my_set.add(4)
print("After adding 4:", my_set)

# Removing elements from the set
my_set.remove(2)
print("After removing 2:", my_set)

# Since sets do not support indexing, we cannot directly modify an element,
# but we can remove and add elements to simulate modification
my_set.remove(1)
my_set.add(10)
print("After modifying 1 to 10:", my_set)
